{
  "name": "sbronsted/librest",
  "type": "library",
  "description": "A resource based rest api",
  "license": "MIT",
  "homepage": "https://github.com/sorenbronsted/librest",
  "authors": [
    {
      "name": "Søren Brønsted",
      "email": "soren@bronsted.dk",
      "homepage": "https://bronsted.dk",
      "role": "Developer"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit -c test-conf.xml",
    "coverage": "vendor/bin/phpunit -c test-conf.xml --coverage-html build/coverage"
  },
  "autoload": {
    "psr-4": { "sbronsted\\": ["rest/"] }
  },
  "require": {
