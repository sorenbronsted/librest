    "sbronsted/libutil": "^2.0",
    "sbronsted/libcommon": "^3.1",
    "ext-json": "*"
  },
  "require-dev": {
    "ruckusing/ruckusing-migrations": "^1.0",
    "sbronsted/libtypes": "^2.1",
    "sbronsted/libdatabase": "^2.0",
    "phpunit/phpunit": "^8.5"
  },
  "repositories": [
    {"type": "composer","url": "http://dev.ufds.org/satis"}
  ]
